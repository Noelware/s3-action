        "mime-types": "3.0.1",
        "zod": "3.25.67"
    },
    "devDependencies": {
        "@augu/eslint-config": "5.3.1",
        "@augu/tsconfig": "1.2.0",
        "@stylistic/eslint-plugin": "5.0.0",
        "@types/bun": "1.2.17",
        "@types/mime-types": "3.0.1",
        "@types/node": "24.0.6",
        "eslint": "9.30.0",
        "eslint-config-prettier": "10.1.5",
        "eslint-formatter-codeframe": "7.32.1",
        "prettier": "3.6.2",
        "typescript-eslint": "8.35.0",
        "typescript": "5.8.3"
